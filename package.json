{
  "name": "sound-parameters-extractor",
  "version": "2.0.2",
  "description": "Calculation of sound parameters",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "code-style": "xo",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "pre-commit": ["code-style"],
  "keywords": [
    "mfcc",
    "mel",
    "scale",
    "cepstrum",
    "coefficients",
    "frequency",
    "fft",
    "audio",
    "sound",
    "speech",
    "zcr",
    "zero",
    "crossing",
    "rate",
    "srf",
    "spectral",
    "roll",
    "off",
    "rolloff",
    "point",
    "sc",
    "centroid",
    "delta",
    "delta-delta"
  ],
  "license": "MIT",
  "repository": "waxo/sound-parameters-extractor",
  "dependencies": {
    "dct": "^0.1.0",
    "fft-js": "^0.0.12",
    "fs-extra": "^10.0.1",
    "node-wav": "^0.0.2"
  },
  "devDependencies": {
    "pre-commit": "^1.2.2",
    "xo": "^0.48.0"
  },
  "xo": {
    "space": 2,
    "prettier": true,
    "rules": {
      "unicorn/no-fn-reference-in-iterator": 0,
      "unicorn/no-array-callback-reference": 0,
      "unicorn/no-array-method-this-argument": 0,
      "unicorn/no-reduce": 0
    },
    "envs": [
      "mocha"
    ]
  },
  "prettier": {
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "none"
  }
}
